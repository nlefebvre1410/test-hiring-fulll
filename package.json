{
  "name": "test-hiring-fulll",
  "version": "1.0.0",
  "description": "fulll company hiring test",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nlefebvre1410/test-hiring-fulll.git"
  },
  "author": "nicolas lefebvre",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nlefebvre1410/test-hiring-fulll/issues"
  },
  "homepage": "https://github.com/nlefebvre1410/test-hiring-fulll#readme",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "watch:build": "tsc -p tsconfig.json -w"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "@vitest/coverage-v8": "^3.0.5",
    "cspell": "^4.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "prettier": "^2.1.1",
    "standard-version": "^9.0.0",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.test.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true
  }
}
